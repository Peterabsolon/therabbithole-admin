{
  "presets": ["next/babel"],
  "plugins": [
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false
      }
    ],
    [
      "module-resolver",
      {
        "root": ["./"]
      }
    ]
  ],
  "env": {
    "development": {
      "plugins": ["inline-dotenv"]
    },
    "production": {
      "plugins": ["inline-dotenv"]
    },
    "test": {
      "presets": [["env", { "modules": "commonjs" }], "next/babel"],
      "plugins": [["dynamic-import-node"]]
    }
  }
}
